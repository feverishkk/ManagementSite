<AuthorizeView>

    <Authorized>
        <MudPaper>
            <MudNavMenu>

                <MudNavGroup Title="Home">
                    <MudNavLink Match="NavLinkMatch.All" Href="/">Home</MudNavLink>
                </MudNavGroup>

                <MudNavGroup Title="Profile">
                    <MudNavLink Match="NavLinkMatch.All" Href="changepassword">Change Password</MudNavLink>
                </MudNavGroup>

                <MudNavGroup Title="Managers">
                    <MudNavLink Match="NavLinkMatch.All" Href="managerlist">Manager List</MudNavLink>
                </MudNavGroup>

                <MudNavGroup Title="Customers">
                    <MudNavLink Match="NavLinkMatch.All" Href="customerlist">Customer List</MudNavLink>
                </MudNavGroup>

                <MudNavGroup Title="Items">
                    <MudNavLink  Match="NavLinkMatch.All" Href="itemlist">Total Item List</MudNavLink>

                    <MudNavGroup Title="Weapon List">
                         <MudNavLink Match="NavLinkMatch.All" Href="weapon/dagger">Dagger</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="weapon/onehandbow">One Hand Bow</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="weapon/onehandstick">One Hand Stick</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="weapon/onehandsword">One Hand Sword</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="weapon/twohandbow">Two Hand Bow</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="weapon/twohandstick">Two Hand Stick</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="weapon/twohandsword" Style="color:red;">Two Hand Sword</MudNavLink>
                    </MudNavGroup>

                    <MudNavGroup Title="Equipment List">
                         <MudNavLink Match="NavLinkMatch.All" Href="eqp/armor" Style="color:red;">Armor</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="eqp/cape">Cape</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="eqp/globe">Globe</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="eqp/guard">Guard</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="eqp/helmet">Helmet</MudNavLink>
                    </MudNavGroup>

                    <MudNavGroup Title="Accessories List">
                         <MudNavLink Match="NavLinkMatch.All" Href="acc/belt" Style="color:red;">Belt</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="acc/earring">EarRing</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="acc/neckless">Neckless</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="acc/ring1">Ring 1</MudNavLink>
                         <MudNavLink Match="NavLinkMatch.All" Href="acc/ring2">Ring 2</MudNavLink>
                    </MudNavGroup>
                </MudNavGroup>

                <MudNavGroup Title="Statistics">
                    <MudNavLink Match="NavLinkMatch.All" Href="userchart">Chart</MudNavLink>
                </MudNavGroup>

                <MudNavGroup Title="Log">
                    <MudNavLink Match="NavLinkMatch.All" Href="log">Log</MudNavLink>
                </MudNavGroup>

            </MudNavMenu>
        </MudPaper>
    </Authorized>

    <NotAuthorized>
        <MudNavLink Match="NavLinkMatch.All" Href="login">Login</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="register">Register</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Href="forgotpassword">Forgot Password</MudNavLink>
    </NotAuthorized>
</AuthorizeView>

